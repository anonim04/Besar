<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.StatisticController">
   <children>
      <TableView fx:id="statisticView" editable="true" layoutX="40.0" layoutY="74.0" prefHeight="253.0" prefWidth="507.0" tableMenuButtonVisible="true">
          <columns>
              <TableColumn prefWidth="43.0" text="No">
                  <cellValueFactory>
                      <PropertyValueFactory property="nomor" />
                  </cellValueFactory>
              </TableColumn>
              <TableColumn  prefWidth="94.0" text="Tanggal">
                  <cellValueFactory>
                      <PropertyValueFactory property="date" />
                  </cellValueFactory>
              </TableColumn>
              <TableColumn  prefWidth="195.0" text="Uraian">
                  <cellValueFactory>
                      <PropertyValueFactory property="about" />
                  </cellValueFactory>
              </TableColumn>
              <TableColumn text="Pemasukan">
                  <cellValueFactory>
                      <PropertyValueFactory property="income" />
                  </cellValueFactory>
              </TableColumn>
              <TableColumn  prefWidth="81.0" text="Pengeluaran">
                  <cellValueFactory>
                      <PropertyValueFactory property="spend" />
                  </cellValueFactory>
              </TableColumn>
          </columns>
      </TableView>
   </children>
</AnchorPane>
